  "scripts": {
    "add-licenses": "./bin/add-licenses",
    "build": "gulp build",
    "check-licenses": "./bin/check-licenses",
    "clean": "git clean -x -f",
    "dev": "gulp build && cd build && npm link && cd .. && gulp watch",
    "dist": "gulp dist",
    "fix-format": "node_modules/.bin/tslint -p . --fix && node_modules/.bin/tsfmt -r",
    "lint": "gulp lint",
    "prepare": "npm run snyk-protect",
    "preversion": "npm test",
    "release": "release-it",
    "release-major": "npm run release -- major",
    "release-minor": "npm run release -- minor",
    "release-patch": "npm run release -- patch",
    "snyk-protect": "snyk protect",
    "test": "gulp test",
    "update-submodules": "git submodule update --init --recursive --jobs 4",
    "view-report": "open coverage/index.html",
    "watch": "gulp watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg -p angular",
      "pre-push": "npm test"
    }
  },
  "snyk": true
